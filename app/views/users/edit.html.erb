<br>
<ul>
<%=display_error_messages_if_any(@user)%>
</ul>
<br>

<%= form_with(model: @user) do |f| %>
        <%= render partial: "form1", locals: {f: f} %>
        <% if !@user.cvo %>
            <%= render partial: "form2", locals: {f: f} %>
        <% end %>
        <%= f.submit :update%>
    <% end %>