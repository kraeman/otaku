<h1>Edit an Actor</h1>
<br>
<ul>
<%=display_error_messages_if_any(@actor)%>
</ul>
<br>

<div id="actor-form" class="hide">
    <%= form_with(model: @actor) do |f| %>
        <%= render partial: "form", locals: {f: f} %>
        <%= f.submit :update%>
    <% end %>
    <%= delete_if_admin(@actor)%>
</div>